{
  "name": "grzegorzdrozd/opentelemetry-auto-redis",
  "description": "OpenTelemetry auto-instrumentation for Redis",
  "keywords": ["opentelemetry", "otel", "open-telemetry", "tracing", "redis", "instrumentation"],
  "type": "library",
  "homepage": "https://opentelemetry.io/docs/php",
  "readme": "./README.md",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "colinmollenhour/credis": "^1.16",
    "ext-opentelemetry": "*",
    "guzzlehttp/guzzle": "^7.9",
    "guzzlehttp/psr7": "^2.7",
    "open-telemetry/api": "^1.0",
    "open-telemetry/exporter-otlp": "^1.2",
    "open-telemetry/sem-conv": "^1.24",
    "php": "^8.2",
    "symfony/polyfill-mbstring": "^1.31",
    "symfony/cache": "^7.2"
  },
  "suggest": {
    "ext-redis": "*",
    "predis/predis":"*",
    "phpredis/phpredis":"*"
  },
  "require-dev": {
    "ext-redis": "*",
    "friendsofphp/php-cs-fixer": "^3",
    "open-telemetry/sdk": "^1.0",
    "phan/phan": "^5.0",
    "php-http/mock-client": "*",
    "phpredis/phpredis": "^6.1",
    "phpstan/phpstan": "^1.1",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpunit/phpunit": "^9.5",
    "predis/predis": "^2.3",
    "psalm/plugin-phpunit": "^0.18.4",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "OpenTelemetry\\Contrib\\Instrumentation\\Redis\\": "src/"
    },
    "files": [
      "_register.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OpenTelemetry\\Tests\\Instrumentation\\Redis\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false,
      "tbachert/spi": true
    }
  }
}
